<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddCompanyController">
   <children>
      <Label layoutX="52.0" layoutY="34.0" stylesheets="@MainStyle.css" text="Company Setup: Enter company name." AnchorPane.topAnchor="34.0">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <VBox layoutX="52.0" layoutY="88.0" prefHeight="126.0" prefWidth="100.0">
         <children>
            <Label text="Name: ">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="163.0" layoutY="88.0" prefHeight="123.0" prefWidth="144.0">
         <children>
            <TextField fx:id="companyName" onAction="#createCompanyClicked" prefWidth="100.0">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Button layoutX="211.0" layoutY="231.0" mnemonicParsing="false" onAction="#createCompanyClicked" text="Create New Company" />
   </children>
</AnchorPane>
