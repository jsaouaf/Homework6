<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddProjectController">
   <children>
      <GridPane layoutX="12.0" layoutY="75.0" prefHeight="233.0" prefWidth="264.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="146.0" minWidth="10.0" prefWidth="146.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="106.0" minWidth="10.0" prefWidth="95.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name:" />
            <Label text="Start Date:" GridPane.rowIndex="1" />
            <Label text="Deadline" GridPane.rowIndex="2" />
            <TextField fx:id="projectName" prefHeight="31.0" prefWidth="123.0" GridPane.columnIndex="1">
               <padding>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </padding>
            </TextField>
            <DatePicker fx:id="startDate" prefHeight="31.0" prefWidth="106.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <DatePicker fx:id="deadline" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="*Required" textFill="#eb0c0c" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <Label layoutX="14.0" layoutY="14.0" text="Add New Project" textFill="#1a1e94">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Label>
      <Button layoutX="92.0" layoutY="333.0" mnemonicParsing="false" onAction="#addProjectClicked" text="Add Project" />
      <Button layoutX="209.0" layoutY="11.0" mnemonicParsing="false" onAction="#dashboardClicked" text="Dashboard" />
      <Label layoutX="267.0" layoutY="82.0" text="*" textFill="#e80505" />
      <Label layoutX="267.0" layoutY="123.0" text="*" textFill="#e80505" />
      <Label layoutX="267.0" layoutY="169.0" text="*" textFill="#e80505" />
   </children>
</AnchorPane>
