<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NewTaskController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="New Task" textFill="#0a0a8d">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="31.0" layoutY="61.0" text="Name" />
      <Label layoutX="31.0" layoutY="106.0" text="Hours" />
      <TextField fx:id="taskName" layoutX="98.0" layoutY="55.0" prefHeight="39.0" prefWidth="183.0" promptText="task name" />
      <TextField fx:id="taskHours" layoutX="98.0" layoutY="106.0" prefHeight="39.0" prefWidth="183.0" promptText="estimated task hours">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></TextField>
      <Button layoutX="112.0" layoutY="168.0" mnemonicParsing="false" onAction="#createTaskClicked" text="Create Task" />
      <Button layoutX="234.0" layoutY="8.0" mnemonicParsing="false" onAction="#dashboardClicked" text="Dashboard" />
   </children>
</AnchorPane>
